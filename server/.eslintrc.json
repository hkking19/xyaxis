{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends":[
         "eslint:recommended",
         "airbnb",
         "prettier",
         "plugin:node/recommended"
        ],
    "plugins": [
        "prettier"
    ],    
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier": "warn",
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off",
        "quotes":"error",
        "no-process-exit": "off",
        "object-shorthand": "off",
        "class-methods-use-this": "off",
        "jsx-quotes": "warn"
    }
}
