{
    "trailingComma": "es5",
    "endOfLine": "crlf",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "arrowParens": "always",
    "useTabs": true,
    "jsxSingleQuote": true,
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "parser": "babel",
    "htmlWhitespaceSensitivity": "strict",
    "requirePragma": false,
    "insertPragma": false,
    "embeddedLanguageFormatting": "auto"
}